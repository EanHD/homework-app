# Production Environment Configuration
# This file contains production-specific environment variables
# DO NOT commit sensitive values - use environment-specific secrets

# Application Configuration
VITE_ENVIRONMENT=production
VITE_VERSION=1.0.0
VITE_BUILD_DATE=2025-09-13

# Supabase Configuration (Production)
# Replace with actual production values
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_production_anon_key

# OAuth Provider Configuration
# Google OAuth 2.0
VITE_GOOGLE_OAUTH_CLIENT_ID=your_google_client_id.apps.googleusercontent.com
VITE_GOOGLE_OAUTH_ENABLED=true

# Apple Sign-In
VITE_APPLE_SERVICE_ID=your.apple.service.id
VITE_APPLE_SIGNIN_ENABLED=true

# Performance Configuration
VITE_PERFORMANCE_MONITORING_ENABLED=true
VITE_PERFORMANCE_SAMPLING_RATE=0.1
VITE_ERROR_TRACKING_ENABLED=true

# Deployment Configuration
VITE_BASE_URL=/homework-app/
VITE_GITHUB_PAGES_DOMAIN=username.github.io

# Feature Flags
VITE_OAUTH_INTEGRATION_ENABLED=true
VITE_PREMIUM_UI_ENABLED=true
VITE_ADVANCED_NOTIFICATIONS_ENABLED=true
VITE_ONBOARDING_TOUR_ENABLED=true

# Development Features (Disabled in Production)
VITE_DEBUG_MODE=false
VITE_CONSOLE_LOGGING=false
VITE_MOCK_DATA_ENABLED=false
VITE_DEV_TOOLS_ENABLED=false